msgstr "WordPress errore sul database %1$s per la query %2$s fatta da %3$s"

#. Not gettexted string WP_I18N_DB_QUERY_ERROR
#: wp-includes/wp-db.php:932
msgid "WordPress database error %1$s for query %2$s"
